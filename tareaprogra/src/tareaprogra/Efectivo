package tareaprogra;

import java.util.Date;
public class Efectivo extends Pago{
    private float pagoDelCliente;
    public Efectivo(float p,float m, Date f){
        super(m,f);
        pagoDelCliente= p;
    }
    public float calcDevolucion(float p){
        
        if(p<0){
            p = p *-1;
        }
        return p;
    }
}
